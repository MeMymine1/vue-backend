{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/index.js"
      },
      {
        "src": "/collection/:collectionName",
        "dest": "/index.js"
      },
      {
        "src": "/collection/:collectionName/:id",
        "dest": "/index.js"
      },
      {
        "src": "/collection/:collectionName/search",
        "dest": "/index.js"
      },
      {
        "src": "/images/(.*)",
        "dest": "/index.js"
      }
    ]
  }
  